{
    "manifest_version": 2,
    "name": "TabTimer",
    "version": "1.0",
    "description": "Displays how long each tab has been open",
    "permissions": [
      "tabs",
      "storage"
    ],
    "browser_action": {
      "default_icon": "icons/icon.svg"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "css": ["styles.css"],
        "js": ["content.js"]
      }
    ]
  }